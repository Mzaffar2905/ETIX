<div class="container" >
    <h2>Welcome <%= current_user.first_name %></h2>
    <h5>NIC Number: <%= current_user.nic_number %></h5>

    <%= simple_form_for(@fine) do |f| %>
      <%= f.input :fineticket, input_html: { class: "input" } %>
      <%= f.input :registration_number, input_html: { class: "input" } %>
      <%= f.submit %>
    <% end %>
</div>
